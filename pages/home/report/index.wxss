page {
    padding-top: 50px;
    padding-bottom:170rpx;
}
.cu-custom{
    position: absolute;
    top: 0px;
    width: 100%;
}
.box{
    position: absolute;
    top: 64px;
    width: 100%;
}
.cu-custom .cu-bar{
    padding-right:inherit;
    background-image:linear-gradient(45deg, #ff9700, #ed1c24);
}
.bg-gradual-green {
    background-image:linear-gradient(45deg, #ff9700, #ed1c24);
}
.text-green, .line-green, .lines-green {
    color:#e45c5c;
}
.hidden-ele {
    visibility: hidden;
}
.btn-reset{
    color: rgb(255, 255, 255);
    background-image:linear-gradient(45deg, rgb(189, 184, 184), rgb(193, 193, 193));
    right: 10px;
}
.btn-finish{
    background-image: linear-gradient(45deg, #ff9700, #df575c);
    color: #fff;
    left: 23px;
}
.info-container {
    padding: 15rpx;
}


